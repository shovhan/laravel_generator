{
  "name": "shovhan/laravel-generator",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "authors": [{"name": "Stas Shovhan", "email": "stas.shovhan@gmail.com"}],
  "require": {
    "php": "^7.1",
    "illuminate/support": "^5.6|^6|^7",
    "illuminate/console": "^5.6|^6|^7",
    "illuminate/database": "^5.6|^6|^7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.0",
    "vimeo/psalm": "^3.11"
  },
  "autoload": {
    "psr-4": {
      "Shovhan\\Generator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shovhan\\Generator\\Tests\\": "tests"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shovhan\\Generator\\GeneratorServiceProvider"
      ]
    }
  }
}
